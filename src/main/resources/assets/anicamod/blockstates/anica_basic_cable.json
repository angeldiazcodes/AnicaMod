      "apply": {
        "model": "anicamod:block/anica_basic_cable_extension",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "anicamod:block/anica_basic_cable_extension",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "anicamod:block/anica_basic_cable_extension",
